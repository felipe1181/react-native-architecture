    "react": "18.0.0",
    "react-native": "0.69.1",
    "react-native-config": "^1.4.6",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-toast-message": "^2.1.5",
    "react-promise-tracker": "^2.1.0",
    "styled-components": "^5.3.5",
    "tinygradient": "^1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
